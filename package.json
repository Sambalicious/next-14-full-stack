{
  "name": "new-next-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --cache --ext .js,.ts,.tsx . --fix",
    "lint:fix": "eslint --fix .",
    "postinstall": "husky install",
    "db:generate": "dotenv drizzle-kit generate",
    "db:introspect": "dotenv drizzle-kit introspect",
    "db:push": "dotenv drizzle-kit push",
    "db:migrate": "dotenv tsx src/db/migrate.ts",
    "db:drop-migration": "drizzle-kit drop",
    "db:seed": "dotenv tsx src/db/seed.ts",
    "db:studio": "dotenv drizzle-kit studio"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "arctic": "^1.9.1",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "drizzle-orm": "^0.31.2",
    "eslint": "8.43.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "14.2.4",
    "framer-motion": "^10.12.18",
    "lucia": "^3.2.0",
    "lucide-react": "^0.252.0",
    "next": "14.2.4",
    "nextjs-toploader": "^1.6.12",
    "oslo": "^1.2.1",
    "postcss": "8.4.24",
    "postgres": "^3.4.4",
    "react": "18.3.1",
    "react-day-picker": "^8.8.0",
    "react-dom": "18.3.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.3",
    "zod": "^3.23.8",
    "zsa": "^0.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.22.8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tsx": "^4.16.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  }
}
